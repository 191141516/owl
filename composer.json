{
    "name": "yeaha/owl",
    "description": "RESTFul Web Framework",
    "keywords": ["framework", "mvc", "swoole", "restful", "orm", "datamapper"],
    "homepage": "https://github.com/yeaha/owl",
    "license": "MIT",
    "authors": [
        {
            "name": "YangYi",
            "email": "yangyi.cn.gz@gmail.com",
            "homepage": "https://github.com/yeaha"
        }
    ],
    "require": {
        "php": ">=5.6.0",
        "psr/http-message": "^1.0",
        "psr/log": "1.0.0",
        "psr/cache": "1.0.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^5.1.0"
    },
    "suggest": {
        "monolog/monolog": "Allows more advanced logging of the application flow"
    },
    "archive": {
        "exclude": ["/examples", "/tests"]
    },
    "autoload": {
        "psr-4": {"Owl\\": "src"},
        "files": ["src/constant.php", "src/functions.php"]
    }
}
